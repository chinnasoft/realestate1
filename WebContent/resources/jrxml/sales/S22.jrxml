		<queryString>
			<![CDATA[SELECT
     sales_products.`pid` AS sales_products_pid,
     sales_products.`invoice_no` AS sales_products_invoice_no,
     sales_products.`product_id` AS sales_products_product_id,
     sales_products.`product_name` AS sales_products_product_name,
     sales_products.`rate_per_quantity` AS sales_products_rate_per_quantity,
     sales_products.`quantity` AS sales_products_quantity,
     sales_products.`amount` AS sales_products_amount,
     sales_products.`tax` AS sales_products_tax,
     sales_products.`tax_amount` AS sales_products_tax_amount,
     sales_products.`toatal_amount` AS sales_products_toatal_amount,
     sales_products.`status` AS sales_products_status,
     item_tbl.`itemcode` AS item_tbl_itemcode,
     item_tbl.`parentname` AS item_tbl_parentname,
     item_tbl.`itemname` AS item_tbl_itemname,
     item_tbl.`description` AS item_tbl_description,
     item_tbl.`uom` AS item_tbl_uom
FROM
     `sales_details` sales_details INNER JOIN `sales_products` sales_products ON sales_details.`invoice_no` = sales_products.`invoice_no`
     INNER JOIN `item_tbl` item_tbl ON sales_products.`product_id` = item_tbl.`itemcode`
WHERE
     sales_details.`invoice_no` = $P{qid}]]>
		</queryString>
